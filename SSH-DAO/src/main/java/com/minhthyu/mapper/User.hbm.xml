<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.minhthyu.entity.User" table="user">
        <id name="uId" type="java.lang.String" column="uId" length="11"></id>
        <property name="uPassword" type="java.lang.String" column="uPassword" length="20"></property>
        <property name="uStatus" type="int" column="uStatus"></property>
        <property name="uLoginTime" type="java.util.Date" column="uLoginTime"></property>
    </class>
</hibernate-mapping>
